#include<stdio.h>
int main() {
    char ch;
    printf("Enter a Character:\n");
    scanf("%c", &ch);
    if (ch >= 'A' && ch <= 'Z')
        printf("The character entered is in uppercase");
    else if (ch >= 'a' && ch <= 'z')
        printf("The character entered is in lowercase");
    else
        printf("The character entered is not an alphabet");
}
