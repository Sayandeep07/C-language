#include<stdio.h>
int main() {
    int a, b;
    printf("Enter the value of A:\n");
    scanf("%d", &a);
    printf("Enter the value of B:\n");
    scanf("%d", &b);
    printf("Addition = %d\n", a + b);
    printf("Subtraction = %d\n", a - b);
    printf("Multiplication = %d\n", a * b);
    printf("Division = %d\n", b / a);

    return 0;
}
