#include <stdio.h>
#include <string.h>
int main(){
char str1[50],str2[50],str3[50],str4[50];
printf("Enter the First strings: \n");
scanf("%s",&str1);
printf("Enter the Second strings: \n");
scanf("%s",&str2);
printf("Enter the Third strings: \n");
scanf("%s",&str3);
printf("Enter the Fourth strings: \n");
scanf("%s",&str4);

strcat(str1,str2);
strcat(str3,str4);
strcat(str1,str3);
printf("%s",str1);
}
