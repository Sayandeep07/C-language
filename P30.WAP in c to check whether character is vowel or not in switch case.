#include<stdio.h>
int main(){
char a;
printf("Enter character:\n");
scanf("%c",&a);
switch(a){
case 'a':
printf("It is a Vowel");
break;
case 'e':
printf("It is a Vowel");
break;
case 'i':
printf("It is a Vowel");
break;
case 'o':
printf("It is a Vowel");
break;
case 'u':
printf("It is a Vowel");
break;
default:
printf("It is a Consonant");
}
}
